import math

a = int(input())
b = int(input())

factors = []

for i in range(1, int(math.sqrt(a)) + 1):
    if a % i == 0:
        factors.append(i)
        
        if i != a // i:
            factors.append(a // i)


factors.sort()

result = factors[b - 1] if b <= len(factors) else 0


print(result)
