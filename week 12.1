import sys

input_data = sys.stdin.read().split()

X = int(input_data[0])
shoe_sizes = list(map(int, input_data[1:X+1]))

inventory = {}
for size in shoe_sizes:
    if size in inventory:
        inventory[size] += 1
    else:
        inventory[size] = 1

N = int(input_data[X+1])
requests = input_data[X+2:]

total_revenue = 0

for i in range(N):
    size = int(requests[2 * i])
    price = int(requests[2 * i + 1])

    if size in inventory and inventory[size] > 0:
        total_revenue += price
        inventory[size] -= 1

print(total_revenue)
